{
  "expo": {
    "name": "Music Player",
    "slug": "Music Player",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/favicon-1.png",
    "scheme": "MusicPlayer",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/images/splash-1.png",
      "resizeMode": "contain",
      "backgroundColor": "#000"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/favicon-1.png",
        "backgroundColor": "#000"
      },
      "package": "com.abdonaser.music_player_V2"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon-1.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "f0fe9053-ebe5-48c5-9254-6a7af68e5dfd"
      }
    }
  }
}
